{
    "type": "mysql",
    "host": "localhost",
    "port": 3306,
    "username": "root",
    "password": "",
    "database": "node-api",
    "synchronize": false,
    "logging": false,
    "entities": [
        "src/main/domain/model/*.ts"
    ],
    "migrations": [
        "src/migration/*.ts"
    ],
    "subscribers": [
        "src/subscriber/**/*.ts"
    ],
    "extra": {
        "max": 10,
        "min": 5
    },
    "cli": {
        "entitiesDir": "src/main/domain/model",
        "migrationsDir": "src/migration",
        "subscribersDir": "src/subscriber"
    }
}